{
  "compilerOptions": {
   
    "target": "es5",                          
    "module": "commonjs",                     
    "sourceMap": true,                   
    "strict": true,                          
    "esModuleInterop": true ,                  
    "outDir" : "jsfiles",
    "types" : ["jasmine", "node"]
  },

  "typedocOptions": {
    "mode": "modules",
    "out": "docs"
  },
  
  "exclude": [
    "node_modules",
    "asyncAwait",
    "plugins.ts"
  ]
}